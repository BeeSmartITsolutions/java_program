public class Read {
	public static void main(String s[]){
		int a, b,c;
		
		Scanner x = new Scanner(System.in);
		
		System.out.println("Enter the first number: ");
		a = InputStream.nextInt();
		
		System.out.println("Enter the second number: ");
		b = InputStream.nextInt();
		
